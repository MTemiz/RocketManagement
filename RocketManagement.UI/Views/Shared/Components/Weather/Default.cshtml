@using RocketManagement.Application.Features.Weather.Dtos
@model WeatherDto

<div class="card">
    <div class="card-header">
        Weather
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-sm-3 border border-secondary">Temperature</div>
            <div class="col-sm-3 border border-secondary"> @Html.DisplayFor(model => model.Temperature)</div>
            <div class="col-sm-3 border border-secondary">Humidity</div>
            <div class="col-sm-3 border border-secondary">@Html.DisplayFor(model => model.Humidity)</div>
            <div class="col-sm-3 border border-secondary">Pressure</div>
            <div class="col-sm-3 border border-secondary">@Html.DisplayFor(model => model.Pressure)</div>
            <div class="col-sm-3 border border-secondary">Time</div>
            <div class="col-sm-3 border border-secondary">@Html.DisplayFor(model => model.Time)</div>
        </div>
    </div>
</div>
